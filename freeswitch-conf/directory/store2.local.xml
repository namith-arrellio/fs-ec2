<include>
  <domain name="store2.local">
    <params>
      <param name="dial-string" value="{^^:sip_invite_domain=${dialed_domain}:presence_id=${dialed_user}@${dialed_domain}}${sofia_contact(*/${dialed_user}@${dialed_domain})}"/>
    </params>
    <groups>
      <group name="default">
        <users>
          <!-- Users are fetched from XML CURL, but domain presence needs this structure -->
        </users>
      </group>
    </groups>
  </domain>
</include>