<configuration name="fifo.conf" description="FIFO Configuration">
  <settings>
    <param name="delete-all-outbound-member-on-startup" value="false"/>
  </settings>
  <fifos>
    <!-- Store 1 parking slots -->
    <fifo name="700@store1.local" importance="0"/>
    <fifo name="701@store1.local" importance="0"/>
    <fifo name="702@store1.local" importance="0"/>
    <!-- Store 2 parking slots -->
    <fifo name="700@store2.local" importance="0"/>
    <fifo name="701@store2.local" importance="0"/>
    <fifo name="702@store2.local" importance="0"/>
  </fifos>
</configuration>